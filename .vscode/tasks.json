{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "clippy",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo clippy",
			"args": [
				"--workspace",
				"--all-targets",
				"--all-features",
				"--all"
			]
		},
	]
}